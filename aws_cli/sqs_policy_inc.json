{
    "Version": "2012-10-17",
    "Id": "example-ID",
    "Statement": [
      {
        "Sid": "example-statement-ID",
        "Effect": "Allow",
        "Principal": {
          "Service": "s3.amazonaws.com"
        },
        "Action": "SQS:SendMessage",
        "Resource": "arn:aws:sqs:us-east-1:${AWS_ACCOUNT}:${SQS_INC}",
        "Condition": {
          "ArnLike": {
            "aws:SourceArn": "arn:aws:s3:us-east-1:${AWS_ACCOUNT}:${BUCKET_INC}"
          }
        }
      }
    ]
  }